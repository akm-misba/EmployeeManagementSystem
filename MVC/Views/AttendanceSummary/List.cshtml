@model IEnumerable<MVC.Models.AttendanceSummary>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model =>model.Company)
            </th>
            <th>
             @Html.DisplayNameFor(model =>model.Employee)
            </th>
           
            <th>
                @Html.DisplayNameFor(model => model.dtMonth)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dtYear)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Present)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Late)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Absent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Holiday)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        


        @foreach (var item in Model) {
        <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Compname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Empname)
                </td>

            <td>
                @Html.DisplayFor(modelItem => item.dtMonth)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dtYear)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Present)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Late)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Absent)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Holiday)
            </td>
           
        </tr>
}
    </tbody>
</table>

